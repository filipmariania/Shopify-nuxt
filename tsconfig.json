{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "lib": ["dom", "esnext", "esnext.asynciterable"],
    "allowJs": true,
    "outDir": "./lib",
    "skipLibCheck": true,
    "rootDir": "./src",
    "types": ["node", "jest", "@nuxt/vue-app"],
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
